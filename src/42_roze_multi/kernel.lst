   274 00000E39 89E5                <1> 		mov		ebp, esp						; EBP+ 0| EBP（元の値）
   275                              <1> 												; ---------------
   276 00000E3B 68B4000000          <1> 		push	dword 180						;    - 4| dword i = 180;
   277                              <1> 
   278                              <1> 		;---------------------------------------
   279                              <1> 		; FPUを使った処理
   280                              <1> 		;
   281                              <1> 		; A(振幅), k(n/d),r(度→ラディアン)を
   282                              <1> 		; FPU内のレジスタにスタックしておく
   283                              <1> 		;---------------------------------------
   284                              <1> 												; ---------+---------+---------|---------|---------|---------|
   285                              <1> 												;       ST0|      ST1|      ST2|      ST3|      ST4|      ST5|
   286                              <1> 												; ---------+---------+---------|---------|---------|---------|
   287 00000E40 D9EB                <1> 		fldpi									;   pi     |xxxxxxxxx|xxxxxxxxx|xxxxxxxxx|xxxxxxxxx|xxxxxxxxx|
