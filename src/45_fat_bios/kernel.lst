   240                              <1> ;	バラ曲線：初期化
   241                              <1> ;------------------------------------------------------------------------
   242                              <1> ;	バラ曲線を描画するためにFPUのレジスタを初期化する
   243                              <1> ;
   244                              <1> ;	Z = A * sin(nθ)
   245                              <1> ;	  = A * sin( (n/d) * ((π/180) * t) )
   246                              <1> ;
   247                              <1> ;	このグラフを描画するために、X/Y座標を次の様に計算する
   248                              <1> ;
   249                              <1> ;	x = A * sin(nθ) * cos(θ)
   250                              <1> ;	y = A * sin(nθ) * sin(θ)
   251                              <1> ;
   252                              <1> ;	この時、kは(n / d)で指定する。また、ラディアン = 度 * (π / 180)
   253                              <1> ;	である事から、r = π / 180を先に計算しておく
   254                              <1> ;========================================================================
   255                              <1> ;■書式		: void fpu_rose_init(A, n, d);
   256                              <1> ;
   257                              <1> ;■引数
   258                              <1> ;	DWORD	: A
   259                              <1> ;	DWORD	: n
   260                              <1> ;	DWORD	: d
   261                              <1> ;
   262                              <1> ;■戻り値	: 無し
   263                              <1> ;************************************************************************
   264                              <1> fpu_rose_init:
   265                              <1> 		;---------------------------------------
   266                              <1> 		; 【スタックフレームの構築】
   267                              <1> 		;---------------------------------------
   268                              <1> 												; ------|--------
   269                              <1> 												;    +16| d
   270                              <1> 												;    +12| n
   271                              <1> 												;    + 8| A
   272                              <1> 												; ---------------
   273 00000EA0 55                  <1> 		push	ebp								; EBP+ 4| EIP（戻り番地）
   274 00000EA1 89E5                <1> 		mov		ebp, esp						; EBP+ 0| EBP（元の値）
   275                              <1> 												; ---------------
   276 00000EA3 68B4000000          <1> 		push	dword 180						;    - 4| dword i = 180;
   277                              <1> 
   278                              <1> 		;---------------------------------------
   279                              <1> 		; FPUを使った処理
   280                              <1> 		;
   281                              <1> 		; A(振幅), k(n/d),r(度→ラディアン)を
   282                              <1> 		; FPU内のレジスタにスタックしておく
   283                              <1> 		;---------------------------------------
   284                              <1> 												; ---------+---------+---------|---------|---------|---------|
   285                              <1> 												;       ST0|      ST1|      ST2|      ST3|      ST4|      ST5|
   286                              <1> 												; ---------+---------+---------|---------|---------|---------|
   287 00000EA8 D9EB                <1> 		fldpi									;   pi     |xxxxxxxxx|xxxxxxxxx|xxxxxxxxx|xxxxxxxxx|xxxxxxxxx|
